{
  "name": "@inottn/fp-utils",
  "version": "0.4.3",
  "packageManager": "pnpm@8.6.10",
  "description": "A js utils library",
  "author": "inottn<https://github.com/inottn>",
  "homepage": "https://github.com/inottn/fp-utils",
  "type": "module",
  "main": "dist/index-cjs.js",
  "module": "dist/index-esm.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index-esm.js",
      "require": "./dist/index-cjs.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint ./src --ext .js,.ts,.mjs,.cjs",
    "prepare": "husky install",
    "prepublish": "npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "nano-staged": {
    "*.{ts,tsx,js,mjs,less}": "prettier --write",
    "*.{ts,tsx,js,mjs}": "eslint --fix"
  },
  "keywords": [
    "js",
    "utils",
    "typescript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inottn/fp-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:inottn/fp-utils.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitest/coverage-v8": "^1.1.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.4",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "typescript": "^5.2.2",
    "vitest": "^1.1.3"
  }
}
